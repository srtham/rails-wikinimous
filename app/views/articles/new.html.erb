<h1>Wikinimous</h1>

<h2>New Article</h2>

<div>
  <%= form_with model: @article do |a| %>
    <%= a.label :title %>
    <%= a.text_field :title %>
    <%= a.label :content %>
    <%= a.text_field :content %>
    <%= a.submit "Create Article" %>
  <% end %>
</div>

<div>
  <%= link_to "Back", articles_path %>
</div>
